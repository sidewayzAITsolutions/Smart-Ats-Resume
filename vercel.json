      "NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY": "pk_live_51RfIgaEXTLOxdWgMsCB81cRlOu0gBnp6ixcFkx0BSZQeZofUOUwK7SgULmu7tWCblN1ie9Ju18pYnSVw6ky73diP00b7ICyKG5",
